schema {
  query: Query
}

type Query {
  someField: Int @constant(json: "88")

  person: Person @constant(json: """{
  "firstName": "John",
  "lastName": "Smith",
  "age": 29
  }""")

  httpbinJson: HttpbinJson @http(url: "https://httpbin.org/json")
}

type Person {
  firstName: String
  lastName: String
  age: Int
}

type HttpbinJson {
  slideshow: Slideshow
}

type Slideshow {
  author: String
  date: String
  slides: [Slide]
  title: String
}

type Slide {
  title: String
  type: String
  items: [String]
}
